<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_itinerary" model="ir.sequence">
            <field name="name">Itinerary sequence</field>
            <field name="code">travel.itinerary.seq</field>
            <field name="prefix">IT/%(y)s/%(month)s/%(day)s/</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
