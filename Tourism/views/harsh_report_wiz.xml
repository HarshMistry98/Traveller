<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_print_report_wizard_form" model="ir.ui.view">
        <field name="name">travel.print.report.form</field>
        <field name="model">travel.print_report</field>
        <field name="arch" type="xml">
            <form>
                <label string="Orders" for="selected_orders"/>
                <field name="selected_orders"/>
                <footer>
                    <button string="PDF" type="object" class="oe_highlight" name="action_pdf"/>
                    <button string="XLSX" type="object" class="oe_highlight" name="action_xlsx"/>
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>